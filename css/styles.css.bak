.btn{
  
  width :30%;
  height : 45px; 
  border : none; 
  background: linear-gradient(to right, #ff9068 0%, #fd746c 100%);
  border-radius : 10px;
  background-size : 200%;
  border-color: white;
  color : white; 
  font-size: 16px;
 
}

.btn_del {
  text-align: center;
  width :24%;
  height : 35px; 
  border : none; 
  background: linear-gradient(to right, #ff9068 0%, #fd746c 100%);
  border-radius : 10px;
  background-size : 200%;
  border-color: white;
  color : white; 
  font-size: 16px;
}

.btn a {color : white; }

.btn:hover,
.btn_del:hover{
  cursor: pointer;
  background: #adadad;
  clear: both; 
}

