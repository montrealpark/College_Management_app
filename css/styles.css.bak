*{
    margin: 0 auto;
	padding : 0; 
    text-decoration:none;
   box-sizing : border-box;
   font-family: "Helvetica"; 
	color: black;   
}
/* HEADER / FOOTER */
nav ul {
	position:absolute;
	top:42px;
	right:10px;

}

table, th, td {
  border: 1px solid;
}


nav li {
    float: right;
    display: inline-block;
    padding: 2px 20px 10px 40px;

}

#branding{
	padding:10px;
}



.header-container{
	background-color:#f1f1f1 ;
}

.footer{
	clear:both;
	text-align:center;
	background-color:#f1f1f1 ;
	
}

.current{
	text-decoration: underline;
}

.video-section{
  
  width:100%;
  margin:0 auto;
}

.video-container{
  background:#dfdfdf;
  position: relative;
  width: 100%;
  height: auto;
  padding-top: 56.25%;
}

iframe{
  z-index: 1;
  top: 0;
  left: 0;
  position: absolute;
  width: 100%;
  height: 100%;
}

